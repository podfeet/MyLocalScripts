				<key>ActionName</key>
				<string>Run AppleScript</string>
				<key>ActionParameters</key>
				<dict>
					<key>source</key>
					<string>on run {input, parameters}	set numSupportedDocuments to 0	repeat with thisItem in input		set thisItem to thisItem as text		set thisItem to quoted form of POSIX path of thisItem		set currentType to do shell script "echo " &amp; thisItem &amp; " | sed 's/\\(.*\\)\\(\\.\\)\\(.*\\)/\\3/g' | tr '[A-Z]' '[a-z]'"		set numSupportedDocuments to (numSupportedDocuments + 1)		if numSupportedDocuments &gt; 25 then			set titleMsg to "Send to Kindle error"			display alert "Maximum of 25 supported documents can be sent at a  time." as warning			return {}		end if	end repeat	return inputend run</string>
