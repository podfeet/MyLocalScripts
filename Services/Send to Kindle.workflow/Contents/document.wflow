					<key>COMMAND_STRING</key>
					<string>#!/usr/bin/perl

if (@ARGV == 0) {
	exit;
}

my $args;
my $file;

$args="-cmd send -launcher shellextension";
$stkApp="/Applications/Send to Kindle/Send to Kindle.app";

foreach (@ARGV) {
    $file="$_";
    $args=$args." -file ".'"'.$file.'"'
}

system("open -n '$stkApp' --args $args");
</string>
